#include<stdio.h>
#include<conio.h>
int main()
{
float faren,cel;
clrscr();
printf("Enter the farenheit temperature\n");
scanf("%f",&faren);
cel=((faren-32.0)*5.0)/9.0;
printf("The celcius temperature is %f",cel);
getch();
return 0;
}

#include<stdio.h>
#include<conio.h>
int main()
{
int a,b;
clrscr();
printf("Enter the two numbers\n");
scanf("%d%d",&a,&b);
if(a>b)
{
printf("%d is greater ",a);
}
else
{
printf("%d is greater",b);
}
getch();
return 0;
}
